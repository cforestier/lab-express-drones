<h2>Available drones</h2>
{{#each drones}}
  <ul>
    <li>name of the drone: <a href="/drones/{{_id}}/edit">{{name}}</a></li>
    <ul>
      <li>number of propellers: {{propellers}}</li>
      <li>maxspeed: {{maxSpeed}}</li>
    </ul>
  </ul>
  <form action="/drones/{{_id}}/delete" method="POST">
    <button>delete</button>
  </form>
{{/each}}